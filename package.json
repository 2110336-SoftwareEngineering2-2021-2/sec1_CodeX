{
  "name": "code-x",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "yarn nx run-many --target=build --parallel --all",
    "build:frontend": "yarn nx run tuto-real-frontend:build --prod",
    "build:backend": "yarn nx run tuto-real-backend:build --prod",
    "start-front": "nx run tuto-real-frontend:serve",
    "start-back": "nx run tuto-real-backend:serve",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@google-cloud/storage": "^5.18.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "axios": "^0.25.0",
    "class-transformer": "^0.5.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "fastify-swagger": "^4.15.0",
    "firebase": "^9.6.6",
    "firebase-admin": "^10.0.2",
    "googleapis": "^95.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "mongoose": "^6.2.1",
    "nestjs-firebase": "^8.2.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-firebase-jwt": "^1.2.1",
    "react": "17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.25.3",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "serve": "^13.0.2",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.0.0",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.4.6",
    "@nrwl/cypress": "13.4.6",
    "@nrwl/eslint-plugin-nx": "13.4.6",
    "@nrwl/jest": "^13.4.6",
    "@nrwl/linter": "13.4.6",
    "@nrwl/nest": "^13.4.6",
    "@nrwl/node": "13.4.6",
    "@nrwl/react": "13.4.6",
    "@nrwl/tao": "13.4.6",
    "@nrwl/web": "13.4.6",
    "@nrwl/workspace": "13.4.6",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "16.11.7",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.3.1",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "babel-jest": "27.2.3",
    "cypress": "^9.1.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.2.3",
    "prettier": "^2.3.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "~4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageDirectory": "<rootDir>/coverage/"
  }
}
